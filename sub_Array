public class subArray {

    public static void subArrayFun(int number[]){

        for(int i = 0; i<number.length; i++){
            for(int j = i; j<number.length; j++){
                System.out.print("{");
                for(int k = i; k<j;k++){
                    System.out.print(number[k]);
                    // System.out.print(" ");
                    int num = number[i] + number[j];
                    // System.out.println("Sum is " + num);
                }
                System.out.println("}");
                ;
            }
            // System.out.print(")");
            // System.out.print("");
        }
    }

    public static void main(String[] args) {
        int number[] = {1, 2 , 3 , 4 ,50};
        subArrayFun(number);
    }
    
}
